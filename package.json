{
	"name": "react-turborepo",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"start": "turbo run start --parallel --output-logs=hash-only",
		"build:package": "turbo run build:package",
		"lint": "turbo run lint --parallel",
		"lint:fix": "turbo run lint:fix --parallel",
		"test:unit:ci": "turbo run test:unit:ci",
		"static-analysis:ci": "turbo run static-analysis:ci",
		"static-analysis:local": "turbo run static-analysis:local",
		"storybook:dev": "turbo run storybook:dev --parallel",
		"storybook:build": "turbo run storybook:build",
		"e2e:ci": "turbo run e2e:ci --filter fe-e2e",
		"e2e:ci:build": "turbo run build --filter fe",
		"e2e:ci:serve": "turbo run preview --filter fe",
		"e2e:ci:init": "pnpm run e2e:ci:build && pnpm run e2e:ci",
		"clean": "turbo run clean && rimraf node_modules"
	},
	"engines": {
		"node": "20.11.1",
		"pnpm": ">=8.14.1"
	},
	"dependencies": {
		"@tanstack/react-query": "5.21.2",
		"axios": "1.6.7",
		"date-fns": "3.3.1",
		"dayjs": "1.11.10",
		"i18next": "23.8.3",
		"i18next-browser-languagedetector": "7.2.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-helmet-async": "2.0.4",
		"react-hook-form": "7.50.1",
		"react-i18next": "14.0.5",
		"react-router-dom": "6.22.1",
		"styled-components": "6.1.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"@testing-library/jest-dom": "6.4.2",
		"@testing-library/react": "14.2.1",
		"@testing-library/user-event": "14.5.2",
		"@types/node": "20.11.1",
		"@types/react": "18.2.56",
		"@types/react-dom": "18.2.19",
		"@vitejs/plugin-react": "4.2.1",
		"@vitest/ui": "1.3.0",
		"cross-env": "7.0.3",
		"dotenv": "16.4.5",
		"env-cmd": "10.1.0",
		"happy-dom": "13.3.8",
		"husky": "8.0.0",
		"lint-staged": "14.0.0",
		"npm-run-all": "4.1.5",
		"pnpm": "8.14.1",
		"prettier": "3.2.5",
		"rimraf": "5.0.1",
		"ts-prune": "0.10.3",
		"turbo": "1.12.4",
		"typescript": "5.3.3",
		"vite": "5.1.3",
		"vite-plugin-svgr": "4.2.0",
		"vite-plugin-top-level-await": "1.4.1",
		"vite-tsconfig-paths": "4.3.1",
		"vitest": "1.3.0"
	}
}
